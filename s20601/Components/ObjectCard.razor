<MudCard>
    <MudLink Href="@url">
    <MudImage Src="favicon.png"
              ObjectFit="ObjectFit.None"
              Alt="image"
              Elevation="0"
              Height="@ImageHeight"
              Width="@ImageWidth" />
    <MudCardContent>
        <MudStack>
                <MudText Typo="Typo.h6" Align="Align.Center">
                    @Title
                    <MudText Typo="Typo.subtitle2">@Subtitle</MudText>
                </MudText>
        </MudStack>
    </MudCardContent>
    </MudLink>
</MudCard>

@code
{
    [Parameter]
    public required string Title { get; set; }
    [Parameter]
    public required string Subtitle { get; set; }
    [Parameter]
    public required string url { get; set; }
    [Parameter]
    public required int ImageHeight { get; set; }
    [Parameter]
    public required int ImageWidth { get; set; }

}